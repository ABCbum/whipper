CLEANFILES = *.py{c,o} command/*.py{c,o} log/*.py{c,o}

moapdir = $(PYTHONLIBDIR)/moap/extern

moap_PYTHON = \
	__init__.py

logdir = $(PYTHONLIBDIR)/moap/extern/log

log_PYTHON = \
	log/__init__.py \
	log/log.py

commanddir = $(PYTHONLIBDIR)/moap/extern/command

command_PYTHON = \
	command/__init__.py \
	command/command.py

EXTRA_DIST = log/test_log.py command/test_command.py
